using namespace std;
#include <iostream>
#include <iomanip> 
#include<cmath>
int main()
{
	long long a, b, c, d;
	cin >> a >> b >> c >> d;
 
	if (a + b * c == d)
	{
		cout << "YES";
	}
	else if (a + b - c == d)
	{
		cout << "YES";
	}
 
 
	else if (a * b + c == d)
	{
		cout << "YES";
	}
	else if (a * b - c == d)
	{
		cout << "YES";
	}
 
 
	else if (a - b + c == d)
	{
		cout << "YES";
	}
	else if (a - b * c == d)
	{
		cout << "YES";
	}
 
	else
	{
		cout << "NO";
	}
}
